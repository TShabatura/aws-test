    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "outDir": "./build",
        "resolveJsonModule": true,
        "noImplicitAny": false,
        "allowSyntheticDefaultImports":true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "allowJs": false
    }
}
